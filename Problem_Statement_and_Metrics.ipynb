# Import necessary libraries
import pandas as pd
import numpy as np

# Load the dataset (Assuming the file is 'aerofit_treadmill.csv')
aerofit = pd.read_csv('aerofit_treadmill.csv')

# 1. Shape of data [cite: 33]
print("Data Shape:", aerofit.shape)

# 2. Attributes [cite: 37]
print("Columns:", aerofit.columns)

# 3. Datatypes [cite: 41]
